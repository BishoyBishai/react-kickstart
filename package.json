{
    "name": "kickstart",
    "version": "1.0.0",
    "description": "kickstart react-redux-typescript-project with jest testing",
    "main": "index.js",
    "scripts": {
        "start": "webpack-dev-server --progress",
        "test": "jest"
    },
    "keywords": ["react", "redux", "typescript", "jest", "kickstart"],
    "author": "Bishoy Bishai",
    "license": "ISC",
    "dependencies": {
        "react": "^16.1.1",
        "react-dom": "^16.1.1",
        "react-redux": "^5.0.6",
        "react-router-dom": "^4.2.2",
        "react-router-redux": "^5.0.0-alpha.6",
        "redux": "^3.7.2",
        "redux-logger": "^3.0.6",
        "redux-promise-middleware": "^5.0.0",
        "redux-thunk": "^2.2.0"
    },
    "devDependencies": {
        "@types/enzyme": "^3.1.5",
        "@types/jest": "^21.1.8",
        "@types/react": "^16.0.25",
        "@types/react-dom": "^16.0.3",
        "@types/react-redux": "^5.0.13",
        "@types/react-router-redux": "^5.0.8",
        "@types/redux": "^3.6.0",
        "awesome-typescript-loader": "^3.4.0",
        "css-loader": "^0.28.7",
        "enzyme": "^3.2.0",
        "enzyme-adapter-react-16": "^1.1.0",
        "file-loader": "^1.1.5",
        "html-webpack-plugin": "^2.30.1",
        "jest": "^21.2.1",
        "node-sass": "^4.7.2",
        "react-test-renderer": "^16.1.1",
        "source-map-loader": "^0.2.3",
        "style-loader": "^0.19.0",
        "typescript": "^2.6.1",
        "webpack": "^3.8.1",
        "webpack-dev-middleware": "^1.12.1",
        "webpack-dev-server": "^2.9.4",
        "webpack-html-plugin": "^0.1.1"
    },
    "jest": {
        "setupFiles": [
            "<rootDir>/test-config/test-shim.js",
            "<rootDir>/test-config/test-setup.js"
        ],
        "moduleFileExtensions": ["ts", "tsx", "js"],
        "transform": {
            "^.+\\.(ts|tsx)$": "<rootDir>/test-config/test-preprocessor.js"
        },
        "testMatch": ["**/__tests__/**/*.(ts|tsx|js)"]
    }
}